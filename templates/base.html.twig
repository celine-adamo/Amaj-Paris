<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Amaj Paris -
			{% block title %}
				{% trans %}Accueil{% endtrans %}
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`#}
		{% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"/>
			<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
			<link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
		{% endblock %}
	</head>

	<body>
		<header>
			<div class="container">
				<div id="demo" class="carousel slide" data-ride="carousel">
					<div class="carousel-inner">
						<div class="carousel-item active">
							<img src="images/bio1.png" alt="Carrousel slide 1" class="d-block w-100">
						</div>
						<div class="carousel-item">
							<img src="images/bio.PNG" alt="Carrousel slide 2" class="d-block w-100">
						</div>
					</div>
					<!-- Contrôles -->
					<a class="carousel-control-prev" href="#demo" role="button" data-slide="prev">
						<span class="carousel-control-prev-icon" aria-hidden="true"></span>
						<span class="sr-only">Précédent</span>
					</a>
					<a class="carousel-control-next" href="#demo" role="button" data-slide="next">
						<span class="carousel-control-next-icon" aria-hidden="true"></span>
						<span class="sr-only">Suivant</span>
					</a>
				</div>
			</div>

			<nav class="navbar-dark bg-dark sticky-top">
				<div id="navigation">

					<div id="menuToggle">
						<input id="menu_mob_imp" type="checkbox">
							<span></span>
							<span></span>
							<span></span>
							<ul id="menu_mob">
								<h4>
									<a href="#">Les icôniques</a>
									<a href="#">Les créations</a>
								</h4>

								<h5>
									<a href="#">Qui sommes-nous</a>
									<a href="#">Politique de retour</a>
									<a href="#">Contactez-nous
										<i class="fas fa-phone-alt"></i>
									</a>

								</h5>

								<h6>
									<a href="#">Restez connecté
										<i class="fab fa-instagram"></i>
										<i class="fab fa-facebook"></i>
									</a>
								</h6>
							</ul>
						</input>
					</div>
					<div class="container">
						<div class="row align-items-start">
							<div class="col-3">
								<a class="nav-link" href="#">LE DRESSING UNISEXE</a>
							</div>
							<div class="col-2">
								<img src="images/logo.PNG" alt="Carrousel slide 2" height="80px" width="auto" class="logo">
							</div>
							<div class="col-3">
								<a class="nav-link" href="#">JE PERSONNALISE</a>
							</div>

							<div class="col-3">
								<span class="icon">
									<i class="fas fa-language"></i>
								</span>
								<span class="icon">
									<i class="fas fa-user"></i>
								</span>
								<span class="icon">
									<i class="fa fa-envelope"></i>
								</span>
								<span class="icon">
									<i class="fa fa-shopping-basket"></i>
								</span>
							</div>

						</div>
					</div>
				</div>
			</nav>
		</header>

				{% block body %}{% endblock %}
			</body>
			{% block javascripts %}
				{# <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.6.0/dist/umd/popper.min.js" integrity="sha384-KsvD1yqQ1/1+IA7gi3P0tyJcT3vR+NdBTt13hSJ2lnve8agRGXTTyNaBYmCR/Nwi" crossorigin="anonymous"></script>
				<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.min.js" integrity="sha384-nsg8ua9HAw1y0W1btsyWgBklPnCUAFLuTMS2G72MMONqmOymq585AcH49TLBQObG" crossorigin="anonymous"></script> #}

				<script src="{{ asset('assets/js/script.js') }}"></script>{#{{ encore_entry_script_tags('app') }}#}{% endblock %}
		</body>
	</html>

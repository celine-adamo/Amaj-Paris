<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Amaj Paris -
			{% block title %}
				{% trans %}Accueil{% endtrans %}
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`#}
		{% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"/>
			<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
			<link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
		{% endblock %}
	</head>

	<body>
		<header>
			<nav class="navbar-dark bg-dark sticky-top">
				<div id="navigation">

					<div id="menuToggle">
						<input id="menu_mob_imp" type="checkbox">
							<span></span>
							<span></span>
							<span></span>

							<ul id="menu_mob">
								<h4>
									<a href="#">Les icôniques</a>
									<a href="#">Les créations</a>
								</h4>

								<h5>
									<a href="#">Qui sommes-nous</a>
									<a href="#">Politique de retour</a>
									<a href="#">Contactez-nous
										<i class="fas fa-phone-alt"></i>
									</a>

								</h5>

								<h6>
									<a href="#">Restez connecté
										<i class="fab fa-instagram"></i>
										<i class="fab fa-facebook"></i>
									</a>
								</h6>
							</ul>
						</input>
					</div>

					<div class="container">
						<div class="row align-items-start">
							<div class="col">
								<a class="nav-link" href="#">LE DRESSING UNISEXE</a>
							</div>
							<div class="col">
								<img src="images/logo.PNG" alt="Carrousel slide 2" height="100px" width="auto" class="logo">
							</div>
							<div class="col">
								<a class="nav-link" href="#">JE PERSONNALISE</a>
							</div>

							<div class="col">
								<span class="icon">
									<i class="fas fa-language"></i>
								</span>
								<span class="icon">
									<i class="fas fa-user"></i>
								</span>
								<span class="icon">
									<i class="fa fa-envelope"></i>
								</span>
								<span class="icon">
									<i class="fa fa-shopping-basket"></i>
								</span>
							</div>
						</div>
					</div>

				</div>
			</nav>
		</body>
	</html>
</header>

{% block body %}{% endblock %}</body>{% block javascripts %}
<script src="{{ asset('assets/js/script.js') }}"></script>{#{{ encore_entry_script_tags('app') }}#}{% endblock %}</html>
